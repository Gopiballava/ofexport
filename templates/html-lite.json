{
    "indent": 2,
    "dateFormat": "%Y-%m-%d",
    "depth" : 1, 
    "nodes": {
        "ProjectStart": "$indent<H$depth class=\"Project\"><a href=\"$link\">$name</a></H$depth><ul>", 
        "FolderStart": "$indent<H$depth class=\"Folder\"><a href=\"$link\">$name</a></H$depth>", 
        "TaskStart": "$indent<li class=\"Task\"><a href=\"$link\">$name</a><span class=\"Attrib\">$date_completed</span></li>", 
        "TaskGroupStart": "$indent<li class=\"TaskGroup\"><a href=\"$link\">$name</a><span class=\"Attrib\">$date_completed</span></li><ul>", 
        "ContextStart": "$indent<H$depth class=\"Context\"><a href=\"$link\">$name</a><H$depth>",
        "ProjectEnd": "$indent</ul>",
        "TaskGroupEnd": "$indent</ul>"
    }, 
    "attributes" : {
    	"name" : {"default": "", "format": "$value", "type": "string"},
    	"id" : {"default": "", "format": " $value", "type": "string"},
    	"type" : {"default": "", "format": " $value", "type": "string"},
    	"flagged" : {"default": "", "format": "  <span class=\"AFlagged\">FLAGGED</span>", "type": "boolean", "eval": "True if value else None"},
    	"context" : {"default": "", "format": " <span class=\"AContext\">$value</span>", "type": "string", "eval": "value.name"},
    	"project" : {"default": "", "format": " <span class=\"AProject\">$value</span>", "type": "string", "eval": "value.name"},
    	"date_due" : {"default": "", "format": " <span class=\"ADue\">$value</span>", "type": "date"},
    	"date_to_start" : {"default": "", "format": " <span class=\"AStart\">$value</span>", "type": "date"},
    	"date_completed" : {"default": "", "format": " <span class=\"AComplete\">$value</span>", "type": "date"},
    	"link" : {"default": "", "format": "$value", "type": "string"},
    	"note" : {"default": "", "format": "$value", "type": "note"},
    	"flagged" : {"default": "", "format": "  <span class=\"AFlagged\">FLAGGED</span>", "type": "boolean", "eval": "True if value else None"}
    },
    "indentString": "  ",
    "preambleFile":"html-start.txt",
    "postamble":"  </body>\n<html>"
}