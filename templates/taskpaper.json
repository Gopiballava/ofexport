{
    "indent": 0,
    "dateFormat": "%Y-%m-%d",
    "depth": 0, 
    "Nodes": {
        "ProjectStart": "$indent$name:$flagged$date_to_start$date_due$date_completed$context$project\n$indent\t$link", 
        "FolderStart": "$indent$name:\n$indent\t$link", 
        "ContextStart": "$indent$name:\n$indent\t$link",
        "TaskStart": "$indent- $name $flagged$date_to_start$date_due$date_completed$context$project", 
        "TaskGroupStart": "$indent$name:$flagged$date_to_start$date_due$date_completed$context$project"
    }, 
    "NodeAttributeDefaults": {
        "date_to_start": "", 
        "date_due": "", 
        "date_completed": "", 
        "link" : "",
        "name": "", 
        "project": "", 
        "context": "", 
        "flagged": ""
    }, 
    "indentString": "\t", 
    "NodeAttributes": {
        "date_to_start": " @start($value)", 
        "date_due": " @due($value)", 
        "date_completed": " @done($value)", 
        "link": "$value", 
        "name": "$value", 
        "project": " @project($value)", 
        "context": " @context($value)", 
        "flagged": " @flagged"
    }
}