{
    "indent": 2,
    "depth" : 1, 
    "Nodes": {
        "ProjectStart": "$indent<H$depth>Project: $name $flagged$date_to_start$date_due$date_completed$context$project</H$depth><ul>", 
        "FolderStart": "$indent<H$depth>Folder $name</H$depth>", 
        "TaskStart": "$indent<li>Task $name $flagged$date_to_start$date_due$date_completed$context$project</li>", 
        "TaskGroupStart": "$indent<li>TaskGroup $name $flagged$date_to_start$date_due$date_completed$context$project</li><ul>", 
        "ContextStart": "$indent<H$depth>Context $name<H$depth>",
        "ProjectEnd": "$indent</ul>",
        "TaskGroupEnd": "$indent</ul>"
    }, 
    "NodeAttributeDefaults": {
        "date_to_start": "", 
        "date_due": "", 
        "date_completed": "", 
        "name": "", 
        "project": "", 
        "context": "", 
        "flagged": ""
    }, 
    "indentString": "  ", 
    "NodeAttributes": {
        "date_to_start": " <b>start:$value</b>", 
        "date_due": " <b>due:$value</b>", 
        "date_completed": " <b>done:$value</b>", 
        "name": "$value", 
        "project": " <b>project:$value</b>", 
        "context": " <b>context:$value</b>", 
        "flagged": " <b>flagged</b>"
    },
    "preamble":"<html>\n  <head>\n    <title>OmniFocus</title>\n  </head>\n  <body>",
    "postamble":"  </body>\n<html>"
}