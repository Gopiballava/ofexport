{
    "indent": 0,
    "dateFormat": "%Y-%m-%d",
    "depth" : 0, 
    "nodes": {
        "ProjectStart": "$hashes$name", 
        "FolderStart": "$hashes$name", 
        "ContextStart": "$hashes$name",
        "TaskStart": "$indent- $name$date_completed", 
        "TaskGroupStart": "$indent- $name$date_completed",
        "NoteLine": "$indent    $note_line"
    }, 
    "attributes" : {
    	"name" : {"default": "", "format": "$value", "type": "string"},
    	"id" : {"default": "", "format": " $value", "type": "string"},
    	"type" : {"default": "", "format": " $value", "type": "string"},
    	"flagged" : {"default": "", "format": " flagged", "type": "boolean", "eval": "True if value else None"},
    	"context" : {"default": "", "format": " project:**$value**", "type": "string", "eval": "value.name"},
    	"project" : {"default": "", "format": " context:**$value**", "type": "string", "eval": "value.name"},
    	"date_due" : {"default": "", "format": " due:**$value**", "type": "date"},
    	"date_to_start" : {"default": "", "format": " start:**$value**", "type": "date"},
    	"date_completed" : {"default": "", "format": " done:**$value**", "type": "date"},
    	"link" : {"default": "", "format": "$value", "type": "string"},
    	"note" : {"default": "", "format": "$value", "type": "note"},
    	"hashes": {"default": "", "format": "$value", "type": "string"}
    },
    "indentString": "    "
}